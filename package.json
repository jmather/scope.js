{
  "name": "scope.js",
  "version": "1.0.0",
  "description": "A tool for handling data transformations",
  "main": "index.js",
  "scripts": {
    "start": "open www/startup.html && DEBUG=express:* ./bin/dev-server.js",
    "pretest": "npm run-script lint",
    "test": "mocha --recursive tests -r should -r sinon || echo ''",
    "coverage": "istanbul cover --report html _mocha -- --recursive -R spec -r should tests && open coverage/index.html",
    "loc": "sloc lib/",
    "docs": "jsdoc -r lib/ -d docs && open docs/index.html",
    "lint": "jshint -c jshint.json lib --reporter=./node_modules/jshint-full-path/index.js",
    "complexity": "rm -rf complexity && mkdir complexity && plato -r -d complexity lib && open complexity/index.html",
    "complexity-simple": "rm -rf complexity.* && find lib/ | grep .js | xargs ./node_modules/.bin/jsc --report complexity.json"
  },
  "author": "Jacob Mather",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmather/scope.js.git"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "dependencies": {
    "amdefine": "^1.0.0",
    "cli": "^0.9.0",
    "flux": "^2.0.3",
    "immutable": "^3.7.4",
    "keymirror": "^0.1.1",
    "object-assign": "^3.0.0",
    "pretty-data": "^0.40.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "browserify": "^6.2.0",
    "envify": "^3.0.0",
    "express": "^4.13.3",
    "istanbul": "^0.3.17",
    "jsc": "^0.3.11",
    "jsdoc": "^3.3.2",
    "jshint": "^2.8.0",
    "jshint-full-path": "^1.1.1",
    "mocha": "^2.2.5",
    "plato": "^1.5.0",
    "react": "^0.13.3",
    "reactify": "^0.15.2",
    "serve-static": "^1.10.0",
    "should": "^7.0.2",
    "sinon": "^1.15.4",
    "sloc": "^0.1.9",
    "uglify-js": "~2.4.15",
    "watchify": "^2.1.1"
  }
}
